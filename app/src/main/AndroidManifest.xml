<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.jit.sose.eschool">

    <!--网络权限-->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/elogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/elogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CourseActivity"/>

        <activity android:name=".activity.LoginActivity_"/>
        <activity android:name=".activity.PersonalActivity_"/>

    </application>

</manifest>